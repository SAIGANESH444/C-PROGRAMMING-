// THREE LARGEST NUMBERS

#include<stdio.h>

void main()

{
 int num1,num2,num3;

 printf("enter the num1");
 scanf("%d",&num1);

 printf("enter the num2");
 scanf("%d",&num2);

 printf("enter the num3");
 scanf("%d",&num3);

 if( num1 >= num2 && num1 >= num3)
   printf("num1 : largest");

   else
   if(num2 >= num1 && num2 >= num3)
   printf("num2 : largest");

   else
    if(num3 >= num1 && num3 >= num2)
    printf("num3 : largest");

}

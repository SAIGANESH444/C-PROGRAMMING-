// avg of array

#include<stdio.h>

void main()
{
int i,avg,total,count=0;
int a[10];

srand(time(0));

for(i=0;i<10;i++)
{
    a[i] = rand()%100;
    printf("%5d",a[i]);
}
   for(i=0;i<10;i++)
    {
     total+=a[i];
     count++;
    avg = total /a[i];
    }

  printf("total     : %d\n",total);
  printf("avg      : %d\n",total/count);
}

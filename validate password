// validate password

#include <conio.h>

void main()
{
int i,upper=0,digit=0;
char ch;

printf("enter password =");

for(i=1;i<=6;i++)
{
ch = getch();
   putch('*');
      if(isdigit(ch))
         digit = 1;
else
if(isupper(ch))
upper = 1;
}
if(upper && digit)
printf("\n VALID PASSWORD");
else
printf("\n INVALID PASSWORD");
}
